<?xml version="1.0" encoding="UTF-8"?>
<prestashop xmlns:xlink="http://www.w3.org/1999/xlink">
    <product_feature_value>
        <id>{{prestashopId}}</id>
        <id_feature xlink:href="{{basepath}}product_features/{{feature}}">{{feature}}</id_feature>
        <custom>{{custom}}</custom>
        <value>
            <language id="{{language}}" xlink:href="{{basepath}}languages/{{language}}">{{value}}</language>
        </value>
    </product_feature_value>
</prestashop>